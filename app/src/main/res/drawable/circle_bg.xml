<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:id="@+id/selectable_kachel_shape">
        <shape android:shape="ring" android:thicknessRatio="2" android:useLevel="false" android:innerRadius="0dp">
            <gradient android:startColor="@color/colorPrimary" android:endColor="@color/colorPrimary" android:type="sweep" android:centerColor="@color/colorPrimary"/>
        </shape>
    </item>
    <item>
        <shape android:shape="ring" android:thicknessRatio="2.2" android:useLevel="false" android:innerRadius="0dp">
            <solid android:color="@color/colorPrimary"/>
            <padding android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp"/>
        </shape>
    </item>
</layer-list>
